{
     "parser": "babel-eslint",
     "extends": [
       "airbnb"
     ],
     "env": {
       "browser": true,
       "node": true
     },
     "rules": {
       "no-console": "off",
       "comma-dangle": "off",
       "react/jsx-filename-extension": "off",
       "quotes": [2, "double", { "avoidEscape": true }],
     }
}